{
    "name":"freyo/xinge",
    "description":"This package makes it easy to send notifications via Tencent Xinge with Laravel",
    "license":"MIT",
    "authors":[
        {
            "name":"freyo",
            "email":"freyhsiao@gmail.com"
        }
    ],
    "require":{
        "php":">=5.6.4",
        "illuminate/notifications":"5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support":"5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "require-dev":{
        "mockery/mockery":"^0.9.5",
        "phpunit/phpunit":"5.*"
    },
    "autoload":{
        "psr-4":{
            "Freyo\\Xinge\\":"src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freyo\\Xinge\\ServiceProvider"
            ]
        }
    }
}
